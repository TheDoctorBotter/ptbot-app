# PTBot Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Key for PTBot chat functionality
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# YouTube API Key for exercise video integration
EXPO_PUBLIC_YOUTUBE_API_KEY=your_youtube_api_key_here

# Dr. Justin Lemmo's YouTube Channel ID
EXPO_PUBLIC_YOUTUBE_CHANNEL_ID=UCYourChannelIdHere

# Email service configuration (for verification emails)
EXPO_PUBLIC_RESEND_API_KEY=your_resend_api_key_here

# App URL for email links (production URL)
EXPO_PUBLIC_APP_URL=https://your-app-domain.com

# From email address for notifications
EXPO_PUBLIC_FROM_EMAIL=noreply@yourdomain.com